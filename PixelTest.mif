-- Altera Memory Initialization File (MIF)

DEPTH = 2048;
WIDTH = 16;

ADDRESS_RADIX = HEX;
DATA_RADIX = HEX;

CONTENT
  BEGIN
    [000..7FF] : 0000;  -- Default to NOP

           000 : B800;  -- 		LOADI 0
           001 : 98B0;  -- 		OUT PXL_A 
           002 : B800;  -- 		LOADI &HF000
           003 : 98B1;  -- 		OUT PXL_D
  END;
