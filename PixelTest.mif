-- Altera Memory Initialization File (MIF)

DEPTH = 2048;
WIDTH = 16;

ADDRESS_RADIX = HEX;
DATA_RADIX = HEX;

CONTENT
  BEGIN
    [000..7FF] : 0000;  -- Default to NOP

           000 : 9000;  -- Loop: IN 	 Switches
           001 : 98B2;  --       OUT    ALL_PXLS
           002 : 2800;  -- 	  JUMP   Loop
  END;
