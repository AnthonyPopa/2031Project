-- Altera Memory Initialization File (MIF)

DEPTH = 2048;
WIDTH = 16;

ADDRESS_RADIX = HEX;
DATA_RADIX = HEX;

CONTENT
  BEGIN
    [000..7FF] : 0000;  -- Default to NOP

           000 : 0802;  -- 		LOAD COLOR
           001 : 98B2;  -- 		OUT ALL_PXLS
           002 : FFFF;  -- COLOR: DW &HFFFF
  END;
